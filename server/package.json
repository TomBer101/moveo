{
  "name": "moveo-server",
  "version": "1.0.0",
  "description": "TypeScript Express server with MongoDB Atlas and session management",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:debug": "nodemon --exec ts-node --inspect src/server.ts",
    "dev:debug-brk": "nodemon --exec ts-node --inspect-brk src/server.ts",
    "build": "tsc",
    "start": "node dist/server.ts",
    "clean": "rm -rf dist",
    "test-setup": "node test-setup.js"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "mongoose": "^8.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.0.3",
    "eslint": "^9.29.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
